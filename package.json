{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:root": "vite -- --project=root",
    "build:root": "vite build -- --project=root",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "new:page": "node ./scripts/index.mjs",
    "start:daWoSi": "vite -- --project=daWoSi",
    "build:daWoSi": "vite build -- --project=daWoSi",
    "start:test": "vite -- --project=test",
    "build:test": "vite build -- --project=test"
  },
  "dependencies": {
    "airpower": "^0.0.11",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.3",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "terser": "^5.29.2",
    "typescript": "~5.4.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.5.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.1",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^2.0.7"
  }
}